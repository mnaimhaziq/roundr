<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.layout.VBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.Button?>
<?import javafx.geometry.Insets?>

<VBox prefHeight="700.0" prefWidth="1000.0" alignment="CENTER" spacing="20.0" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.game.roundr.MainMenuController">
    <children>
        <Label text="Main Menu" />
        <Button fx:id="primaryButton" text="Switch to Join Lobby" onAction="#tempSwitch"/>
    </children>
    <padding>
        <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
    </padding>
</VBox>